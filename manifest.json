{
  "manifest_version": 3,
  "name": "Form Autofill Extension",
  "version": "1.0",
  "description": "Fill form inputs with predefined values.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
